{
    "version": 2,
    "routes": [
      {
        "src": "/bios",
        "dest": "/api/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
          }
      },
      {
        "src": "/bios/:frequency",
        "dest": "/api/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
          }
      },
      {
        "src": "/bios/:frequency/:depth/:size/:location/:condition",
        "dest": "/api/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
          }
      }
    ],
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
  }